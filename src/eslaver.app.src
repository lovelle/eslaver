{application, eslaver,
 [{vsn, "1.0.0"},
  {description, "Erlang redis slave replication client/server"},
  {applications, [stdlib, kernel]},
  {registered, [eslaver]},
  {mod, {eslaver, []}},
  {env, [
    {master, [
        {node, "nodeA"},
        {host, "127.0.0.1"},
        {port, 6379}
    ]},
    {rdbcompression, yes},
    {rdbchecksum, yes},
    {dbfilename, "dump.rdb"}
  ]}
 ]}.
